{
  "name": "ushiboy/cors-proxy",
  "scripts": {
    "test": "phpunit",
    "phpunit": "phpunit",
    "phpcs": "phpcs --standard=PSR2 src"
  },
  "autoload": {
    "psr-4": {
      "CORSProxy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CORSProxy\\Test\\": "test/"
    }
  },
  "require": {
    "php": "~7",
    "ext-mbstring": "*",
    "rmccue/requests": "^1.7",
    "vlucas/phpdotenv": "^2.4",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "squizlabs/php_codesniffer": "^3.1",
    "heroku/heroku-buildpack-php": "^126.0"
  }
}
